{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "moduleDetection": "force",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*"]
  }